<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression Test" verbose="1" parallel="tests" thread-count="10">
    <parameter name="browser" value="firefox"/>
    <test name="Test Footer" thread-count="10" >
        <parameter name="cucumber.filter.tags" value="@Footer"/>
        <classes>
            <class name="runner.CucumberRunnerTest" />
        </classes>
    </test>
    <test name="Test Login" thread-count="10" >
        <parameter name="cucumber.filter.tags" value="@Login"/>
        <classes>
            <class name="runner.CucumberRunnerTest" />
        </classes>
    </test>
</suite>

