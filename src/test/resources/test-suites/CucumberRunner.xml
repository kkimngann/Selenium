<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression Test" verbose="1" >
    <test name="Test Order Computer on Chrome" parallel="classes" thread-count="10" >
        <parameter name="browser" value="chrome"></parameter>
        <parameter name="cucumber.filter.tags=" value="@OrderComputer"/>
        <classes>
            <class name="runner.CucumberRunnerTest" />
        </classes>
    </test>
    <test name="Test Footer Chrome" parallel="classes" thread-count="10" >
        <parameter name="cucumber.filter.tags=" value="@Footer"/>
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="runner.CucumberRunnerTest" />
        </classes>
    </test>
</suite>

